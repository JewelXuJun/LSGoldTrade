<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true">
        <shape>
            <solid android:color="@color/common_item_background" />
            <corners android:radius="@dimen/common_btn_corner" />
            <stroke android:width="1dp" android:color="@color/common_btn_yellow_normal" android:dashGap="0dp" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/common_item_background" />
            <corners android:radius="@dimen/common_btn_corner" />
            <stroke android:width="1dp" android:color="@color/common_btn_bg_disable" android:dashGap="0dp" />
        </shape>
    </item>

</selector>